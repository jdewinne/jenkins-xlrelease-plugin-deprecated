<?xml version='1.0' encoding='UTF-8'?>
<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd" xmlns="http://www.xebialabs.com/deployit/synthetic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <type type="jenkins.BuildJob" extends="xlrelease.PythonScript">
        <property name="jenkinsUrl" category="input" label="Jenkins Server URL" />
        <property name="keyFile" category="input" label="Jenkins CLI Key File" />
        <property name="password" password="true" category="input" />
        <property name="jobName" category="input" label="Job Name" />
        <property name="parameters" size="large" category="input" required="false" label="Job Parameters"
          description="Optional job parameters specified in Java Properties file format, one 'key=value' entry per line" />
    </type>
</synthetic>